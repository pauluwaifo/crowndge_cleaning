	 <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            font-family: 'Montserrat', sans-serif;
            overflow-x: hidden;
        }

        .slider-container {
            position: relative;
            width: 100%;
            height: 100vh;
            min-height: 600px;
            overflow: hidden;
        }

        .slide {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            opacity: 0;
            transition: opacity 1s ease-in-out;
            display: flex;
            align-items: center;
            justify-content: center;
            text-align: center;
            color: white;
        }

        .slide.active {
            opacity: 1;
        }

        .slide-content {
            z-index: 10;
            max-width: 1200px;
            padding: 20px;
            position: relative;
        }

        .slide-image {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            object-fit: cover;
            z-index: 1;
        }

        .slide-overlay {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            z-index: 2;
        }

        /* Slide 1 */
        .slide-1 .slide-overlay {
            background: linear-gradient(90deg, rgba(134,143,150,0.8) 0%, rgba(89,97,100,0.8) 100%);
        }

        .slide-1 .slide-content {
            text-align: left;
            margin-left: 5%;
        }

        /* Slide 2 */
        .slide-2 .slide-overlay {
            background: linear-gradient(120deg, rgba(203,186,204,0.8) 0%, rgba(36,57,73,0.8) 100%);
        }

        .slide-2 .slide-content {
            text-align: right;
            margin-right: 5%;
        }

        /* Slide 3 */
        .slide-3 .slide-overlay {
            background: linear-gradient(0deg, rgba(203,186,204,0.8) 0%, rgba(36,57,73,0.8) 100%);
        }

        /* Slide 4 */
        .slide-4 .slide-overlay {
            background: linear-gradient(140deg, rgba(35,21,87,0.8) 0%, rgba(68,16,122,0.8) 28%, rgba(255,19,97,0.8) 67%, rgba(255,248,0,0.8) 100%);
        }

        .subtitle {
            font-size: 2.5rem;
            font-weight: 400;
            letter-spacing: 8px;
            margin-bottom: 20px;
            text-transform: uppercase;
            animation: slideInFromLeft 1s ease-out 0.5s both;
        }

        .main-title {
            font-size: 4.5rem;
            font-weight: 700;
            line-height: 1.2;
            margin-bottom: 40px;
            letter-spacing: 2px;
            animation: slideInFromLeft 1s ease-out 0.7s both;
        }

        .cta-button {
            display: inline-block;
            padding: 15px 40px;
            border: 2px solid white;
            color: white;
            text-decoration: none;
            font-size: 1.5rem;
            letter-spacing: 2px;
            transition: all 0.3s ease;
            animation: slideInFromLeft 1s ease-out 0.9s both;
        }

        .cta-button:hover {
            background-color: white;
            color: black;
        }

        /* Navigation */
        .slider-nav {
            position: absolute;
            bottom: 30px;
            left: 50%;
            transform: translateX(-50%);
            display: flex;
            gap: 15px;
            z-index: 20;
        }

        .nav-dot {
            width: 60px;
            height: 4px;
            background: rgba(255, 255, 255, 0.5);
            cursor: pointer;
            transition: all 0.3s ease;
        }

        .nav-dot.active {
            background: rgba(255, 255, 255, 1);
        }

        .nav-arrows {
            position: absolute;
            top: 50%;
            transform: translateY(-50%);
            background: rgba(255, 255, 255, 0.2);
            border: none;
            color: white;
            font-size: 2rem;
            padding: 15px 20px;
            cursor: pointer;
            transition: all 0.3s ease;
            z-index: 20;
        }

        .nav-arrows:hover {
            background: rgba(255, 255, 255, 0.4);
        }

        .nav-prev {
            left: 30px;
        }

        .nav-next {
            right: 30px;
        }

        /* Animations */
        @keyframes slideInFromLeft {
            from {
                opacity: 0;
                transform: translateX(-100px);
            }
            to {
                opacity: 1;
                transform: translateX(0);
            }
        }

        @keyframes slideInFromRight {
            from {
                opacity: 0;
                transform: translateX(100px);
            }
            to {
                opacity: 1;
                transform: translateX(0);
            }
        }

        @keyframes slideInFromTop {
            from {
                opacity: 0;
                transform: translateY(-100px);
            }
            to {
                opacity: 1;
                transform: translateY(0);
            }
        }

        @keyframes slideInFromBottom {
            from {
                opacity: 0;
                transform: translateY(100px);
            }
            to {
                opacity: 1;
                transform: translateY(0);
            }
        }

        /* Slide 2 specific animations */
        .slide-2 .subtitle {
            animation: slideInFromRight 1s ease-out 0.5s both;
        }

        .slide-2 .main-title {
            animation: slideInFromRight 1s ease-out 0.7s both;
        }

        .slide-2 .cta-button {
            animation: slideInFromRight 1s ease-out 0.9s both;
        }

        /* Slide 3 specific animations */
        .slide-3 .subtitle {
            animation: slideInFromTop 1s ease-out 0.5s both;
        }

        .slide-3 .main-title {
            animation: slideInFromTop 1s ease-out 0.7s both;
        }

        .slide-3 .cta-button {
            animation: slideInFromTop 1s ease-out 0.9s both;
        }

        /* Slide 4 specific animations */
        .slide-4 .subtitle {
            animation: slideInFromBottom 1s ease-out 0.5s both;
        }

        .slide-4 .main-title {
            animation: slideInFromBottom 1s ease-out 0.7s both;
        }

        .slide-4 .cta-button {
            animation: slideInFromBottom 1s ease-out 0.9s both;
        }

        /* Mobile Responsive Styles */
        @media (max-width: 768px) {
            .slider-container {
                min-height: 500px;
                height: 100vh;
            }

            .slide-content {
                padding: 15px;
                text-align: center !important;
                margin: 0 !important;
            }

            .subtitle {
                font-size: 1.5rem;
                letter-spacing: 4px;
                margin-bottom: 15px;
            }

            .main-title {
                font-size: 2.5rem;
                line-height: 1.1;
                margin-bottom: 25px;
                letter-spacing: 1px;
            }

            .cta-button {
                padding: 12px 25px;
                font-size: 1.1rem;
                letter-spacing: 1px;
            }

            .nav-arrows {
                font-size: 1.5rem;
                padding: 10px 15px;
            }

            .nav-prev {
                left: 15px;
            }

            .nav-next {
                right: 15px;
            }

            .slider-nav {
                bottom: 20px;
                gap: 10px;
            }

            .nav-dot {
                width: 40px;
                height: 3px;
            }
        }

        @media (max-width: 480px) {
            .slider-container {
                min-height: 450px;
            }

            .slide-content {
                padding: 10px;
            }

            .subtitle {
                font-size: 1.2rem;
                letter-spacing: 2px;
                margin-bottom: 10px;
            }

            .main-title {
                font-size: 2rem;
                margin-bottom: 20px;
            }

            .cta-button {
                padding: 10px 20px;
                font-size: 1rem;
            }

            .nav-arrows {
                font-size: 1.2rem;
                padding: 8px 12px;
            }

            .nav-prev {
                left: 10px;
            }

            .nav-next {
                right: 10px;
            }

            .nav-dot {
                width: 30px;
                height: 2px;
            }
        }

        /* Landscape mobile */
        @media (max-width: 768px) and (orientation: landscape) {
            .slider-container {
                min-height: 400px;
            }

            .subtitle {
                font-size: 1.3rem;
                margin-bottom: 8px;
            }

            .main-title {
                font-size: 2.2rem;
                margin-bottom: 15px;
            }
        }
    </style>

